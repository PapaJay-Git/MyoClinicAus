#denies iframe
header set x-frame-options DENY

<Files *.php>
    Order Deny,Allow
    Deny from all
    Allow from 127.0.0.1
</Files>

<Files index.php>
    Order Allow,Deny
    Allow from all
</Files>

<Files *.html>
    Order Deny,Allow
    Deny from all
    Allow from 127.0.0.1
</Files>

<Files *.js>
    Order Deny,Allow
    Deny from all
    Allow from 127.0.0.1
</Files>



